issn = {13640321},
journal = {Renewable and Sustainable Energy Reviews},
keywords = {Fossil fuels,Oil industry,Petroleum,Renewable,Solar},
mendeley-groups = {A - Energ{\'{i}}a Solar/Gasificaci{\'{o}}n Solar},
pages = {296--314},
publisher = {Elsevier},
title = {{Application of solar energy in the oil industry—Current status and future prospects}},
url = {http://www.sciencedirect.com/science/article/pii/S1364032114009617},
