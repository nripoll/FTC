keywords = {Filtration combustion,Reciprocal flow burner,Ultra-low NOx and CO emissions},
mendeley-groups = {A - Hybrid Filtration Combustion},
number = {3},
pages = {570--580},
publisher = {Elsevier Inc.},
title = {{Ultra-low-emission steam boiler constituted of reciprocal flow porous burner}},
url = {http://dx.doi.org/10.1016/j.expthermflusci.2010.11.005},
